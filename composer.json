    "cboden/ratchet": "0.3.6",
    "symfony/console": "^3.2",
    "symfony/yaml": "^3.2"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "2.*"
  },
  "autoload": {
    "psr-4" : {
